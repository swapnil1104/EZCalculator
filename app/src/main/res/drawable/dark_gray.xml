<?xml version="1.0" encoding="utf-8" ?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" android:shape="ring" >
    <item android:state_pressed="true" >
        <shape>
            <solid
                android:color="#a9a9a9" />
            <size android:height="30dp" />
            <stroke android:width=".1dp" android:color="#2A0A0A" />

        </shape>
    </item>
    <item>
        <shape>
            <gradient
                android:startColor="#b4b4b4"
                android:endColor="#a5a5a5"
                android:angle="270" />


        </shape>
    </item>
</selector>
